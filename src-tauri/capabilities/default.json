{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "sql:default",
    "sql:allow-execute",
    "notification:default",
    "os:default",
    "fs:default",
    "fs:write-files",
    "fs:allow-appdata-meta-recursive",
    "dialog:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:4041"
        }
      ]
    }
  ]
}